{
  "expo": {
    "name": "Project 3 Group 8",
    "slug": "project-3-group-8-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "sdkVersion": "49.0.0",
    "icon": "./assets/icon.png",
